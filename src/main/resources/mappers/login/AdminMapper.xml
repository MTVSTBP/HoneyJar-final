<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tbp.honeyjar.login.mapper.admin.AdminMapper">
    <select id="findByAdminId" resultType="com.tbp.honeyjar.login.entity.admin.Admin">
        SELECT *
        FROM admin
        WHERE admin_id = #{adminId}
    </select>

    <select id="findByEmail" resultType="com.tbp.honeyjar.login.entity.admin.Admin">
        SELECT *
        FROM admin
        WHERE email = #{email}
    </select>

    <insert id="insertAdmin" parameterType="com.tbp.honeyjar.login.entity.admin.Admin">
        INSERT INTO admin (email, password)
        VALUES (#{email}, #{password})
    </insert>
</mapper>